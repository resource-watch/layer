{
  "name": "Layer Microservice",
  "tags": [
    "layer"
  ],
  "cache": [
    "layer"
  ],
  "uncache": [
    "layer",
    "dataset"
  ],
  "endpoints": [
    {
      "path": "/v1/layer",
      "method": "GET",
      "redirect": {
        "method": "GET",
        "path": "/api/v1/layer"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer",
      "method": "POST",
      "authenticated": true,
      "redirect": {
        "method": "POST",
        "path": "/api/v1/dataset/:dataset/layer"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer",
      "method": "GET",
      "redirect": {
        "method": "GET",
        "path": "/api/v1/dataset/:dataset/layer"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer/:layer",
      "method": "GET",
      "redirect": {
        "method": "GET",
        "path": "/api/v1/dataset/:dataset/layer/:layer"
      }
    },
    {
      "path": "/v1/layer/:layer",
      "method": "GET",
      "redirect": {
        "method": "GET",
        "path": "/api/v1/layer/:layer"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer/:layer",
      "method": "PATCH",
      "authenticated": true,
      "redirect": {
        "method": "PATCH",
        "path": "/api/v1/dataset/:dataset/layer/:layer"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer/:layer",
      "method": "DELETE",
      "authenticated": true,
      "redirect": {
        "method": "DELETE",
        "path": "/api/v1/dataset/:dataset/layer/:layer"
      }
    },
    {
      "path": "/v1/dataset/:dataset/layer",
      "method": "DELETE",
      "authenticated": true,
      "redirect": {
        "method": "DELETE",
        "path": "/api/v1/dataset/:dataset/layer"
      }
    },
    {
      "path": "/v1/layer/find-by-ids",
      "method": "POST",
      "authenticated": true,
      "redirect": {
        "method": "POST",
        "path": "/api/v1/layer/find-by-ids"
      }
    },
    {
      "path": "/v1/layer/change-environment/:dataset/:env",
      "method": "PATCH",
      "authenticated": true,
      "redirect": {
        "method": "PATCH",
        "path": "/api/v1/layer/change-environment/:dataset/:env"
      }
    },
    {
      "path": "/v1/layer/:layer/expire-cache",
      "method": "DELETE",
      "authenticated": true,
      "redirect": {
        "method": "DELETE",
        "path": "/api/v1/layer/:layer/expire-cache"
      }
    }
  ]
}
